{
  "name": "uniquer",
  "version": "1.0.0",
  "description": "Save equal files only once, avoid duplicatesSave equal files only once, avoid duplicates.",
  "keywords": [
    "unique",
    "equal",
    "files",
    "hash"
  ],
  "author": "Paul Koeck",
  "main": "out/index.js",
  "types": "out/index.d.ts",
  "files": [
    "out/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint src/**/*",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poeck/uniquer.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/poeck/uniquer/issues"
  },
  "homepage": "https://github.com/poeck/uniquer#readme",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "dependencies": {}
}
